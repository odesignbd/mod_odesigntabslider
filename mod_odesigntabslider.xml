			<fieldset name="Slider_Option">
				
				<field
					name="target"
					type="list"
					default="_blank"
					label="Link Target"
					description="Set link target _parent or _blank">
						<option value="_parent">JBROWSERTARGET_PARENT</option>
						<option value="_blank">JBROWSERTARGET_NEW</option>
				</field>
				<field
					name="autoplay"
					type="radio"
					class="btn-group btn-group-yesno"
					true="1"
					false="0"
					default="1"
					label="Autoplay"
					description="Autoplay">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field
					name="Pause_on_hover"
					type="radio"
					class="btn-group btn-group-yesno"
					default="3"
					label="Pause On MouseOver"
					description="Pause On MouseOver">
						<option value="3">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field
					name="keyboard_nav"
					type="radio"
					class="btn-group btn-group-yesno"
					true="1"
					false="0"
					default="1"
					label="Keyboard Navigation"
					description="Enable or Disable Keyboard Navigation">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field
					name="disable_tab_mb"
					type="radio"
					class="btn-group btn-group-yesno"
					default="752"
					label="Disable Tab in Mobile"
					description="You can Disable Tab Bar in Mobile Device">
						<option value="752">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field
					name="slide_direction"
					type="list"
